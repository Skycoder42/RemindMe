<?xml version="1.0" encoding="UTF-8"?>
<SettingsConfig>
	<Entry key="gui/dateformat"
		   type="list"
		   title="&amp;Date Format"
		   tooltip="Select the format to display dates as"
		   default="1">
		<Property key="_list_data" type="string">int</Property>
		<Property key="_list_elements" type="list">
		   <Element type="object">
			   <Property key="name" type="string">Long</Property>
			   <Property key="value" type="number">0</Property>
		   </Element>
		   <Element type="object">
			   <Property key="name" type="string">Short</Property>
			   <Property key="value" type="number">1</Property>
		   </Element>
		   <Element type="object">
			   <Property key="name" type="string">Narrow</Property>
			   <Property key="value" type="number">2</Property>
		   </Element>
		</Property>
	</Entry>
</SettingsConfig>
